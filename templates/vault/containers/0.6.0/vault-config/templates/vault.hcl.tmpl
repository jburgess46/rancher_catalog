backend "{{getv "/services/vault/metadata/backend"}}" {
    address = "consul.consul:8500"
    path = "vault"
    advertise_addr = "{{getv "/services/vault/metadata/protocol"}}://{{getv "/self/host/agent_ip"}}:{{getv "/services/vault/metadata/port"}}"
}
listener "tcp" {
    address = "0.0.0.0:8200"
    tls_disable = 1
}

